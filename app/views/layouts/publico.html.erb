<!DOCTYPE html>
<html>
  <head>
    <title>IV SECITEX</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <%= favicon_link_tag 'icone.png' %>
    <%= render partial: 'layouts/bootstrap' %>
    <script src="https://use.fontawesome.com/21136133a5.js"></script>
    <%= yield :head %>
  </head>
  <body>
    <div class="text-center">
      <%= image_tag 'logosecitex.png' %>
    </div>
    <div class="container content">
      <%= render partial: "layouts/notify" %>
      <%= yield %>
    </div>
  </body>
</html>
